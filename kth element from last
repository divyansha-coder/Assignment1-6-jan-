int k_fromlast(int k , node* &head , node* &tail){
    int count = 0;
    node* &fast = head;
    node* &slow = head;
    
    while(count != k){
        fast = fast->next;
        count++;
    }
    while(fast -> next != NULL){
        fast = fast->next;
        slow = slow->next;
    }
    return slow->data;
};
